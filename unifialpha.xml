<?xml version="1.0" encoding="utf-8"?>
<Container>
  <Beta>false</Beta>
  <Category>HomeAutomation: Network:Management</Category>
  <Name>UniFi ALPHA</Name>
  <Description></Description>
  <Registry>https://registry.hub.docker.com/u/cordtripper/unifialpha/</Registry>
  <Repository>cordtripper/unifialpha</Repository>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Environment/>
  <Networking>
    <Mode>bridge</Mode>
    <Publish>
      <Port>
        <HostPort>8002</HostPort>
        <ContainerPort>8002</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8000</HostPort>
        <ContainerPort>8000</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8001</HostPort>
        <ContainerPort>8001</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8004</HostPort>
        <ContainerPort>8004</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
      <Port>
        <HostPort>8003</HostPort>
        <ContainerPort>8003</ContainerPort>
        <Protocol>tcp</Protocol>
      </Port>
    </Publish>
  </Networking>
  <Data>
    <Volume>
      <HostDir>/mnt/disk/unAPPS/unifialpha/</HostDir>
      <ContainerDir>/var/lib/unifialpha</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <Version></Version>
  <WebUI>https://[IP]:[PORT:8001]/manage</WebUI>
  <Banner>http://i.imgur.com/dD9ub3n.png</Banner>
  <Icon>http://i.imgur.com/dD9ub3n.png</Icon>
</Container>
